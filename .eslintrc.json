{
  "extends": [
    "eslint:recommended",
    "next/core-web-vitals",
    "next",
    "prettier",
    "plugin:prettier/recommended"
  ],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "error",
    "arrow-body-style": "off",
    "prefer-arrow-callback": "off",
    "endOfLine": "auto"
  }
}
